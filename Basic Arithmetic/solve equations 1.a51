//TO PROVE (A-B)^2 = A^2 + B^2 - 2AB WITH A = 12H AND B = 8H
ORG 00H
//A^2 + B^2 -2AB
	MOV A,#12H
	MOV B,#12H
	MUL AB
	MOV R0,A
	MOV R1,B
	CLR A
	MOV A,#08H
	MOV B,#08H
	MUL AB
	ADDC A,R0
	MOV R0,A
	MOV A,B
	ADDC A,R1
	MOV R1,A
	CLR A
	MOV A,#12H
	MOV B,#08H
	MUL AB
	MOV R2,A
	MOV A,#02H
	MOV B,R2
	MUL AB
	MOV R3,A
	MOV A,R0
	SUBB A,R3
	MOV R0,A
	MOV A,R1
	SUBB A,B
	MOV R1,A
//(A-B)^2
	MOV A,#12H
	SUBB A,#08H
	MOV B,A
	MUL AB
	MOV R4,A
	MOV R5,B
END